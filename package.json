{
  "name": "karma-typescript",
  "version": "3.0.7",
  "description": "Simplifying running unit tests with coverage for Typescript projects.",
  "main": "dist/index.js",
  "keywords": [
    "angularjs",
    "angular2",
    "browserify",
    "commonjs",
    "coverage",
    "css modules",
    "docker",
    "istanbul",
    "jasmine",
    "karma",
    "karma plugin",
    "karma-coverage",
    "karma-plugin",
    "mocha",
    "postcss",
    "react",
    "remap",
    "remap-istanbul",
    "transpile",
    "typescript",
    "unit test"
  ],
  "author": "monounity",
  "contributors": [
    "erikbarke",
    "g00fy-",
