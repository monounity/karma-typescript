  "scripts": {
    "dev": "gulp dev --cwd . && karma start",
    "dev:ci": "gulp dev:ci --cwd . && karma start --single-run",
    "dev:ci:phantomjs": "gulp dev:ci --cwd . && karma --browsers PhantomJS start --single-run",
    "dev:ci:angular2": "gulp dev:ci --cwd . && karma start karma.conf.angular2.js --single-run",
    "dev:ci:phantomjs:angular2": "gulp dev:ci --cwd . && karma start karma.conf.angular2.js --browsers PhantomJS --single-run",
    "dev:ci:core": "gulp dev:ci --cwd . && karma start karma.conf.core.js --single-run",
    "dev:ci:phantomjs:core": "gulp dev:ci --cwd . && karma start karma.conf.core.js --browsers PhantomJS --single-run",
    "dev:ci:emptyfile": "gulp dev:ci --cwd . && karma start karma.conf.emptyfile.js --single-run",
    "dev:ci:phantomjs:emptyfile": "gulp dev:ci --cwd . && karma start karma.conf.emptyfile.js --browsers PhantomJS --single-run",
    "dev:ci:no-module": "gulp dev:ci --cwd . && karma start karma.conf.no.module.js --single-run",
    "dev:ci:phantomjs:no-module": "gulp dev:ci --cwd . && karma start karma.conf.no.module.js --browsers PhantomJS --single-run",
    "dev:ci:react": "gulp dev:ci --cwd . && karma start karma.conf.react.js --single-run",
    "dev:ci:phantomjs:react": "gulp dev:ci --cwd . && karma start karma.conf.react.js --browsers PhantomJS --single-run",
    "prepare": "bower install",
    "test": "karma start"
  },
